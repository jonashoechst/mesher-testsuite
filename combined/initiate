#!/bin/bash

TOTAL_NODES="$1"
TOTAL_NODES_ACTIVE="$2"

TIME_VAL="$3"
TEST_SET="$4"
COUNT_OR_TIME="$5"
COUNT_OR_TIME_VAL="$6"
SALT="$7"

nohup $(dirname $0)/initiate-ddf $TOTAL_NODES $TOTAL_NODES_ACTIVE $TIME_VAL $TEST_SET

nohup $(dirname $0)/initiate-df $TOTAL_NODES $TOTAL_NODES_ACTIVE $COUNT_OR_TIME $COUNT_OR_TIME_VAL $TEST_SET

nohup $(dirname $0)/initiate-dm $TOTAL_NODES $TOTAL_NODES_ACTIVE $TIME_VAL

nohup $(dirname $0)/initiate-m $TOTAL_NODES $TOTAL_NODES_ACTIVE $TEST_SET $TIME_VAL $SALT
